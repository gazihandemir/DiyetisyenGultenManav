@using DiyetisyenGultenManav.WebApp.ViewModels
@model  WarningViewModel
@{
    ViewBag.Title = Model.Title;
    Layout = null;
}


<script src="~/Scripts/jquery-3.5.1.min.js"></script>
<div class="text-center">
    <p><h1>@Model.Header</h1></p>
    <p>
        <h2>
            @Model.Title<br />
        </h2>
        @foreach (string msg in Model.Items)
        {
            @msg <br>
        }
    </p>
</div>
@{
    string js = string.Empty;
    if (Model.IsRedirecting)
    {
        js = "setTimeOut(function(){window.location='" + Model.RedirectingUrl + "';},"
            + Model.RedirectingTimeout + ");";
    }
}
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script>
    $(function () {
        @Html.Raw(js);
    });
</script>


