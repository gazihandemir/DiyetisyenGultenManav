@model IEnumerable<DiyetisyenGultenManav.Entities.Diet>

@{
    ViewBag.Title = "Diyetisyen Bilgileri";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Owner.Username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hastalik)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hikaye)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Olcüm)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tahlil)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EkBilgiler)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Owner.Username)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Hastalik)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Hikaye)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Olcüm)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Tahlil)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.EkBilgiler)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
    }

</table>
