@using DiyetisyenGultenManav.Entities.Messages
@model List<ErrorMessageObj>
@{
    ViewBag.Title = "Hesap Geçersiz";
    Layout = null;
}


<script src="~/Scripts/jquery-3.5.1.min.js"></script>
<div class="text-center">
    <p><h1>Yönlendiriliyorsunuz</h1></p>
    <p>
        <h2>
            Hesabınız geçerli değildir.<br />
        </h2>
        @foreach (ErrorMessageObj err in Model)
        {
            @err.Message <br>
        }
    </p>
</div>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script>
    $(function () {
        setTimeout(function () {
            window.location = "/Home/Index";
        }, 8000);
    });
</script>


